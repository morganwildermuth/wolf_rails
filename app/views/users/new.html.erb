<br><br>
<div class="row">
  <div class="pagination-centered">
    <%= form_for(@user) do |f| %>
      <%= f.label :username %>
      <%= f.text_field :username %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.label :password_confirmation %>
      <%= f.password_field :password_confirmation %>

      <p><%= f.submit %></p>
    <% end %>
  </div>
</div>